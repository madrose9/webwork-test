        $g4 = init_graph(-8,-8,8,8,'axes'=>[0,0],'ticks'=>[16,16],'grid'=>[16,16],'size'=>[400,400]);


        $p1 = FEQ("-x -$a for x in &lt;-8,8&gt; using color:blue weight:2 line:dashed");
        $p2 = FEQ("4 for x in &lt;-8,2&gt; using color:blue weight:2");
