  <!-- Exercise 3.15 -->
  <exercise>
    <webwork>
      <pg-code>        
        # If $domain=n, the graphing window is -n &lt; x &lt; n and -n &lt; y &lt; n
        $w = 300;  # graph size
        $tw = 400; # graph size in TeX

        $a = random(1,3,1);
        $b = non_zero_random(-5,5,1);
        $c = random(2,4,1);

        $f1 = "y +x \ge -$a";
        $f2 = "x \le 2 \text{ and } y \ge 4";
