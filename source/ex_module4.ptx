  <exercise>
    <webwork>
      <pg-code>
        Context("Numeric");
        Context()->normalStrings;
        Context()->strings->add(T=>{});
        Context()->strings->add(F=>{});

        $questStr1 = " \ln(x-y) = \ln(x) - \ln(y) ";
        $ansStr1 = "F";
        $questStr2 = " \frac {\ln(x)}{\ln(y)} = \ln(x) - \ln(y) ";
        $ansStr2 = "F";
        $questStr3 = " \log_a(b) = \log_b(a) ";
        $ansStr3 = "F";
        $questStr4 = " (\ln (a))^ b = b \ln(a) ";
        $ansStr4 = "F";
        $questStr5 = " \ln(a^b) = b \ln(a) ";
        $ansStr5 = "T";
