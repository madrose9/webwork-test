        $graph5 = init_graph(-$domain,-$domain,$domain,$domain, axes=>[0,0], grid=>[$twicedomain,$twicedomain]);
        plot_functions( $graph5, "$shuffledQuestions[4] for x in &lt;-$domain,$domain&gt; using color:blue and weight:2");
        $graph5 -> lb(new Label ( $labelpos,0,'x','black','right','top'));
