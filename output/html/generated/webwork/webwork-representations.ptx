$e=random(2,8,2);

$ans=Compute(2**($a)*(2**($b)-2**($c)*(2**($d)+2**($e))));

$step1 = 2**($d);
$step2 = 2**($e);
$step3 = $step1 + $step2;
$step4 = 2**($c)*$step3;
$step5 = 2**($b)*$step4;

